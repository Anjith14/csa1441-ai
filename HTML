<!-- Title Tag -->
<!DOCTYPE html>
<html>
<head>
  <title>My HTML Demo Page</title>
</head>
<body>

<!-- Heading -->
<h1>Welcome to My HTML Tag Demo</h1>

<!-- Anchor Tag -->
<p>Visit <a href="https://www.example.com" target="_blank" title="Go to Example">Example Website</a> for more info.</p>

<!-- Image Tag -->
<img src="https://via.placeholder.com/150" alt="Placeholder Image" />

<!-- Bold & Italic Text -->
<p>This is <b>bold</b> and this is <i>italic</i>.</p>

<!-- Ordered List -->
<ol>
  <li>First Item</li>
  <li>Second Item</li>
</ol>

<!-- Unordered List -->
<ul>
  <li>Apple</li>
  <li>Banana</li>
</ul>

<!-- Line Break -->
<p>Line one.<br>Line two.</p>

</body>
</html>
